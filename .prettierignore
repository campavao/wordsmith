# Ignore build directories
/.next/
/out/

# Ignore public directory if it contains large binary files
/public/

# Ignore coverage directory if you are using testing
/coverage/

# Ignore Node.js modules
/node_modules/

# Ignore any files that are autogenerated
/.env
/.env.local
/.env.*.local

# Ignore files in version control
/.git/
/.svn/
